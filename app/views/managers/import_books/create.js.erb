var id = <% @book_csv.id %>
$(function(){
  $.ajax({
    url: '/book_csv/' + id + '/status',
    type: 'GET',
    timeout: 1000
    }).done(function(html){
      if (@book_csv.status == '完了'){
        $('.polling_result').append('インポート完了');
      } else {
        $('.polling_result').append('インポート中');
      }
      }).fail(function(html){
      $('.polling_result').append('インポート失敗');
    });
});
